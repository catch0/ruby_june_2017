<h1>Listing Dojos!!</h1>
<table>
    <th>
        <tr>
        <td> Branch</td>
        <td> Street</td>
        <td> City </td>
        <td> State</td>
        <td> Actions </td>
        </tr>
    </th>
 <% @dojo.each do |dojo| %>    
    <tr>
    <td> <%= dojo.branch %> </td>
    <td> <%= dojo.street %> </td>
    <td> <%= dojo.city %> </td>
    <td> <%= dojo.state %> </td>
    <td> <a href="/dojos/<%= dojo.id%>">show </a></td>
    <td> <a href="/dojos/edit/<%= dojo.id%>" value="patch" method="post"> edit </a></td>
    <td> <a href="/dojos/destroy/<%= dojo.id%> " value="delete" method="post"> delete </a></td>        
    </tr>
<% end %>
</table>
<a href="/dojos/new" >New Dojo</a>