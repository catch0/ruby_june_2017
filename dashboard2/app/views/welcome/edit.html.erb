<h1> Editing <%= @branch.name %></h1>
<% if flash[:errors] %>
	<% flash[:errors].each do |e| %>
		<p><%= e %></p>
	<% end %>
<% end %>	
<form action= "update/<%= @branch.id%>" method="post">
<input type = "hidden" name="_method" value="patch">
<input type = "hidden" name= "authenticity_token" value = "<%= form_authenticity_token %>">
<label> Name </label>
    <input type="text" name = "name" placeholder="<%= @branch.name %>">
<label> Street </label>
<input type="text" name = "street" placeholder="<%= @branch.street %> ">
<label> City </label>
<input type="text" name = "city" placeholder="<%= @branch.city %> ">    
<labeL> State </labeL>
<input type="text" name = "state" placeholder="<%= @branch.state %>">    
<input type = "submit">
</form>
<%= link_to "Back", root_path %>